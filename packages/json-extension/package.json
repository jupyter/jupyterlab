{
  "name": "@jupyterlab/json-extension",
  "version": "3.1.0-alpha.3",
  "description": "JupyterLab - JSON Renderer",
  "homepage": "https://github.com/jupyterlab/jupyterlab",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab.git"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "sideEffects": [
    "style/*"
  ],
  "exports": {
    ".": "./lib/index.js",
    "./package.json": "./package.json",
    "./style/index.css": "./style/index.css",
    "./style/index.js": "./style/index.js"
  },
  "main": "./lib/index.js",
  "types": "index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "lib/*.d.ts",
    "lib/*.js",
    "style/*"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "docs": "typedoc src",
    "prepublishOnly": "npm run build",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/apputils": "^3.1.0-alpha.3",
    "@jupyterlab/rendermime-interfaces": "^3.1.0-alpha.3",
    "@jupyterlab/translation": "^3.1.0-alpha.3",
    "@jupyterlab/ui-components": "^3.1.0-alpha.3",
    "@lumino/coreutils": "^1.5.3",
    "@lumino/messaging": "^1.4.3",
    "@lumino/widgets": "^1.18.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-highlighter": "^0.4.3",
    "react-json-tree": "^0.12.1"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-highlighter": "^0.3.4",
    "@types/react-json-tree": "^0.6.11",
    "rimraf": "~3.0.0",
    "typedoc": "~0.20.0-beta.27",
    "typescript": "~4.1.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "mimeExtension": true
  },
  "styleModule": "style/index.js",
  "typesVersions": {
    "*": {
      "*": [
        "./lib/*"
      ]
    }
  }
}
